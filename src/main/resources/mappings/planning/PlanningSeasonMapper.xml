<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.base.sbc.module.planning.mapper.PlanningSeasonMapper">

    <!-- ***********************自定义方法区 不替换的区域【other_start】***************************************** -->
        <!--    查询产品季(产品季总览)-->
    <select id="selectProductSeason" resultType="com.base.sbc.module.planning.entity.PlanningSeason">
        select * from t_planning_season s
        ${ ew.customSqlSegment}
        and exists (select id from t_planning_band b  where b.planning_season_id = s.id and del_flag='0' and status='2')
    </select>

    <!-- ***********************自定义方法区 不替换的区域【other_end】******************************************** -->

</mapper>